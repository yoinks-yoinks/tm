{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "tm",
	"main": "./src/worker/index.ts",
	"compatibility_date": "2025-06-17",
	"compatibility_flags": [
		"nodejs_compat"
	],
	"observability": {
		"enabled": true
	},
	"ai": {
		"binding": "AI"
	},
	"kv_namespaces": [
		{
			"binding": "USAGE_KV",
			"id": "f22c2e68f3c04962af4d0af39cf370eb"
		}
	],
	"d1_databases": [
		{
			"binding": "DB",
			"database_name": "tm-d1",
			"database_id": "5e6ea8f0-53b5-4a5b-9834-37154a3256a7",
			"migrations_dir": "drizzle"
		}
	],
	"upload_source_maps": true,
	"assets": {
		"directory": "./dist/client",
		"not_found_handling": "single-page-application"
	}
}